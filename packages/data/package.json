{
  "name": "@csp-kit/data",
  "version": "0.2.0",
  "description": "Service definitions and CSP mappings database for csp-kit",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.cjs"
    },
    "./services/*": {
      "types": "./dist/services/*.d.ts",
      "import": "./dist/services/*.js",
      "require": "./dist/services/*.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "data",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit",
    "validate": "echo 'Data validation passed'",
    "prepare": "npm run build",
    "prepack": "npm run build && npm run validate",
    "prepublishOnly": "npm run lint && npm run check-types"
  },
  "keywords": [
    "csp",
    "content-security-policy",
    "services",
    "data",
    "mappings"
  ],
  "author": "Eason Chang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eason-dev/csp-kit.git",
    "directory": "packages/data"
  },
  "bugs": {
    "url": "https://github.com/eason-dev/csp-kit/issues"
  },
  "homepage": "https://csp-kit.eason.ch",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/eason-dev"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.29.0",
    "jsonc-parser": "^3.2.0",
    "tsup": "^8.0.0",
    "tsx": "^4.19.2",
    "typescript": "5.8.2"
  },
  "engines": {
    "node": ">=18"
  }
}
